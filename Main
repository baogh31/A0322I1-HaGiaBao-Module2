package com.company;

public class Main {
    public static void main(String[] args) {
        Point2D point2D = new Point2D(1.4f, 6.8f);
        Point3D point3D = new Point3D(point2D.getX(), point2D.getY(), 5.7f);
        System.out.println(point2D.toString());
        System.out.println(point3D.toString());
    }
}
