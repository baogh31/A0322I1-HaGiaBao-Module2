package com.company;

import com.company.StopWatch;

import java.util.Arrays;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Nhập số phần tử mảng ");
        int size = scanner.nextInt();
        int[] unsortedArr = new int[size];
        int[] sortedArr = new int[size];
        for (int i = 0; i < size; i++) {
            System.out.print("Nhập phần thử index " + i + " ");
            int num = scanner.nextInt();
            unsortedArr[i] = num;
        }
        StopWatch stopWatch = new StopWatch();
        stopWatch.getStartTime();
        stopWatch.start();
        int max = unsortedArr[0];
        for (int k = 0; k < size - 1; k++) {
            if (unsortedArr[k + 1] > max) {
                max = unsortedArr[k + 1];
            }
        }
        int min = unsortedArr[0];

        for (int j = 0; j < size; j++) {

            for (int h = 0; h < size - 1; h++) {
                if (unsortedArr[h + 1] < min) {
                    min = unsortedArr[h + 1];
                }
            }
            sortedArr[j] = min;
            for (int index = 0; index < size; index++) {
                if (unsortedArr[index] == min) {
                    min = max;
                    unsortedArr[index] = max;
                }
            }
        }
        System.out.println(Arrays.toString(sortedArr));
//        int n=2000000;
//        for (int i=1;i<n;i++){
//            System.out.println(i);
//        }
        // e k biết sao nó chạy cái sort lại ra 0s nữa :"D mà thử cái vòng lặp thì ra thời gian được thật
        stopWatch.getEndTime();
        stopWatch.stop();
        System.out.println("Thời gian thực thi: " + stopWatch.getElapsedTime() + " milisec");
    }
}
